{
    "name": "venveo/craft-boilerplate",
    "type": "project",
    "description": "Craft CMS Boilerplate for spinning up new projects.",
    "keywords": [
        "craft", "craftcms", "boilerplate", "craftboilerplate", "craft boilerplate"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/venveo/craft-boilerplate/issues",
        "source": "https://github.com/venveo/craft-boilerplate"
    },
    "require": {
        "php": "^7.0",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-imagick": "*",
        "predis/predis": "^1.1"
        "craftcms/cms": "^3.0.0",
        "vlucas/phpdotenv": "^2.4.0",
        "craftcms/aws-s3": "^1.0",
        "craftcms/element-api": "^2.5",
        "craftcms/mailgun": "^1.0",
        "craftcms/redactor": "^1.0",
        "charliedev/element-map": "^1.0",
        "mmikkel/cp-field-inspect": "^1.0",
        "pennebaker/craft-architect": "^2.0",
        "nystudio107/craft-eagerbeaver": "^1.0",
        "ether/seo": "^3.2",
        "ostark/upper": "^1.3",
        "aelvan/imager": "^2.0",
        "clubstudioltd/craft-asset-rev": "^6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "5.4.*",
        "phpunit/phpunit-selenium": "^3.0",
        "phpspec/phpspec": "~2.1"
    }
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [],
        "craft:install": []
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
